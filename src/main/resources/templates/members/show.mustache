{{>layouts/header}}
<table class="table">
    <thead>
    <tr>
        <th scope="col">Email</th>
        <th scope="col">Password</th>
    </tr>
    </thead>
    <tbody>
    {{#member}} <!-- 모델 변수명과 같아야함 -->
    <tr>
        <th scope="row">{{email}}</th>
        <td colspan="2">{{password}}</td>
    </tr>
    {{/member}}
    </tbody>
    <a href="/members">Go to member list</a>

</table>
{{>layouts/footer}}